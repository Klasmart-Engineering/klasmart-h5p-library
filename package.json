{
    "name": "kidsloop-h5p-service",
    "version": "2.10.0",
    "description": "KidsLoop H5P Service",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/calmisland/kidsloop-h5p-library"
    },
    "scripts": {
        "start": "cross-env NEW_RELIC_HOME=tests ts-node -r dotenv/config src/express.ts",
        "start:dist": "node dist/express.js",
        "build": "rm -rf dist && npm run build:server && npm run build:client",
        "build:client": "npx tsc -p ./tsconfig.client.json",
        "build:custom": "cross-env NODE_ENV=production webpack",
        "build:server": "npx tsc -p ./tsconfig.server.json && npm run build:custom",
        "test": "npm run test:unit && npm run test:integration",
        "test:unit": "cross-env NEW_RELIC_HOME=tests LOG_STYLE=SILENT mocha --require ts-node/register tests/unit/**/*.test.ts",
        "test:integration": "cross-env DOTENV_CONFIG_PATH=./.env.test mocha --require ts-node/register -r dotenv/config tests/integration/**/*.test.ts --timeout 8000",
        "format:check": "npx prettier --check \"{src,tests}/**/*.ts\"",
        "format": "npx prettier --write \"{src,tests}/**/*.ts\"",
        "lint": "eslint -c .eslintrc.js --ext .ts src --quiet",
        "release": "standard-version",
        "uninstall": "rm -rf node_modules && rm -rf dist",
        "watch:custom": "webpack --watch"
    },
    "dependencies": {
        "@lumieducation/h5p-express": "bitbucket:calmisland/h5p-nodejs-library-packages#h5p-express",
        "@lumieducation/h5p-html-exporter": "^9.0.6",
        "@lumieducation/h5p-mongos3": "bitbucket:calmisland/h5p-nodejs-library-packages#h5p-mongos3",
        "@lumieducation/h5p-redis-lock": "^9.0.5",
        "@lumieducation/h5p-server": "bitbucket:calmisland/h5p-nodejs-library-packages#h5p-server",
        "body-parser": "1.19.0",
        "cache-manager": "^3.6.0",
        "cache-manager-redis-store": "^2.0.0",
        "cookie": "^0.4.1",
        "cors": "^2.8.5",
        "debug": "^4.3.3",
        "dotenv": "^14.3.2",
        "express": "^4.17.2",
        "express-fileupload": "^1.2.1",
        "fs-extra": "^10.0.0",
        "html2canvas": "^1.4.1",
        "i18next": "^20.6.1",
        "i18next-fs-backend": "^1.1.4",
        "i18next-http-middleware": "^3.1.5",
        "ioredis": "^4.28.2",
        "jsonwebtoken": "^8.5.1",
        "kidsloop-nodejs-logger": "bitbucket:calmisland/kidsloop-nodejs-logger#v0.2.16",
        "kidsloop-token-validation": "bitbucket:calmisland/kidsloop-token-validation#v1.4.1",
        "kidsloop-xapi-uploader": "bitbucket:calmisland/h5p-xapi-uploader#v2.2.1",
        "newrelic": "^8.4.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "requirejs": "^2.3.6",
        "tmp-promise": "^3.0.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.0",
        "@babel/core": "^7.17.2",
        "@babel/plugin-transform-destructuring": "^7.16.7",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@commitlint/config-conventional": "^16.0.0",
        "@types/cache-manager-redis-store": "^2.0.1",
        "@types/chai": "^4.3.0",
        "@types/cookie": "^0.4.1",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.13",
        "@types/i18next-fs-backend": "^1.1.2",
        "@types/ioredis": "^4.28.7",
        "@types/jsdom": "^16.2.14",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/mocha": "^9.1.0",
        "@types/newrelic": "^7.0.3",
        "@types/node": "^16.11.7",
        "@types/react": "^16.14.21",
        "@types/react-dom": "^17.0.11",
        "@types/serve-static": "^1.13.10",
        "@types/sinon": "^10.0.8",
        "babel-loader": "^8.2.3",
        "buffer": "^6.0.3",
        "chai": "^4.3.5",
        "commitlint": "9.1.2",
        "cross-env": "^7.0.3",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.26.1",
        "eslint-plugin-react-hooks": "4.2.0",
        "husky": "^4.3.8",
        "jsdom": "^19.0.0",
        "mocha": "^9.2.0",
        "prettier": "2.1.1",
        "sinon": "^12.0.1",
        "standard-version": "^9.3.2",
        "terser-webpack-plugin": "^4.2.3",
        "ts-loader": "^8.3.0",
        "ts-node": "9.0.0",
        "typescript": "4.0.2",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2"
    }
}
