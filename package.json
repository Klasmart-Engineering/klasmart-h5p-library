{
    "name": "kidsloop-h5p-service",
    "version": "2.0.0",
    "description": "KidsLoop H5P Service",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/calmisland/kidsloop-h5p-library"
    },
    "scripts": {
        "start": "ts-node src/express.ts",
        "build": "npm run build:server && npm run build:client",
        "build:webpack": "webpack",
        "build:client": "npx tsc -p ./tsconfig.client.json",
        "build:server": "npx tsc -p ./tsconfig.server.json",
        "format:check": "npx prettier --check \"{src,test}/**/*.ts\"",
        "format": "npx prettier --write \"{src,test}/**/*.ts\"",
        "lint": "eslint -c .eslintrc.js --ext .ts src --quiet",
        "semantic-release": "standard-version",
        "uninstall": "rm -rf node_modules && rm -rf build",
        "get-slack-message": "ts-node src/getSlackMessage.ts"
    },
    "dependencies": {
        "@lumieducation/h5p-express": "bitbucket:calmisland/h5p-nodejs-library-packages#h5p-express",
        "@lumieducation/h5p-html-exporter": "^9.0.6",
        "@lumieducation/h5p-mongos3": "bitbucket:calmisland/h5p-nodejs-library-packages#h5p-mongos3",
        "@lumieducation/h5p-redis-lock": "^9.0.5",
        "@lumieducation/h5p-server": "bitbucket:calmisland/h5p-nodejs-library-packages#h5p-server",
        "aws-sdk": "2.744.0",
        "body-parser": "1.19.0",
        "cache-manager": "^3.6.0",
        "cache-manager-redis-store": "^2.0.0",
        "cookie": "^0.4.1",
        "cors": "^2.8.5",
        "debug": "^4.3.3",
        "express": "4.17.1",
        "express-fileupload": "^1.2.1",
        "fs-extra": "^10.0.0",
        "i18next": "^20.6.1",
        "i18next-fs-backend": "^1.1.4",
        "i18next-http-middleware": "^3.1.5",
        "ioredis": "^4.28.2",
        "jsonwebtoken": "^8.5.1",
        "kidsloop-xapi-uploader": "bitbucket:calmisland/h5p-xapi-uploader#v1.0.1",
        "newrelic": "^8.4.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "requirejs": "^2.3.6",
        "tmp-promise": "^3.0.3"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "9.1.2",
        "@types/cache-manager-redis-store": "^2.0.1",
        "@types/cookie": "^0.4.1",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.2.1",
        "@types/fs-extra": "^9.0.13",
        "@types/i18next-fs-backend": "^1.1.2",
        "@types/ioredis": "^4.28.7",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/newrelic": "^7.0.3",
        "@types/react": "^16.14.21",
        "@types/react-dom": "^17.0.11",
        "commitlint": "9.1.2",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.26.1",
        "eslint-plugin-react-hooks": "4.2.0",
        "husky": "4.2.5",
        "prettier": "2.1.1",
        "standard-version": "^9.3.2",
        "terser-webpack-plugin": "^4.2.3",
        "ts-loader": "^8.3.0",
        "ts-node": "9.0.0",
        "tslint": "6.1.3",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "typescript": "4.0.2",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12"
    }
}
